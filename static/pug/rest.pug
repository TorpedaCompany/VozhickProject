            if (dataRest && dataRest != undefined)
              #rest-catalog-dish.row.rest-catalog-dish
                mixin dish(item)
                  .rest-card-dish(data-category-dish= item.category data-id-dish= item._id)
                    .closeBtn
                      i.fa.fa-times(aria-hidden='true')
                    .card-dish-img
                      img.b-lazy(data-src= item.img, src='../image/rest/rest_placeholder.svg')
                    .card-dish-content
                      .card-dish-description
                        span= item.name
                        p= item.description
                          p= 'Состав: ' + item.composition
                        p
                      .card-dish-footer
                        .card-dish-info
                          .dish-grams= item.grams + ' гр.'
                          .dish-price= item.price.toFixed(2) + ' руб.'
                        .card-dish-action
                          .inpMinus -
                          input.dishCount(type='number', step='1', min='1', max='50', name='dishCount', value='1', readonly='')
                          .inpPlus +
                          .addToCard
                            i.fa.fa-inverse.fa-shopping-cart(aria-hidden='true')
                for item, index in dataRest.restDishes
                  +dish(item)
                if dataRest.restConstructor
                  if(dataRest.constructorPancake.length > 0)
                    .rest-card-dish-constructor.rest-card-dish
                      .closeBtn
                          i.fa.fa-times(aria-hidden='true')
                      .card-dish-img
                          img.b-lazy(data-src='../image/constructor/constr_pancake.png', src='../image/rest/rest_placeholder.svg')
                      .card-dish-content
                          a(href='../rests/'+dataRest.restName+'/constrPancake')
                              .card-dish-button-constructor
                                  .dish-button-constructor
                                      span.fa.fa-cube
                                      | Создать свой блинчик
                  if(dataRest.constructorPizza.length > 0)
                    .rest-card-dish-constructor.rest-card-dish
                      .closeBtn
                          i.fa.fa-times(aria-hidden='true')
                      .card-dish-img
                          img.b-lazy(data-src='../image/constructor/constr_pizza.png', src='../image/rest/rest_placeholder.svg')
                      .card-dish-content
                          a(href='../rests/'+dataRest.restName+'/constrPizza')
                              .card-dish-button-constructor
                                  .dish-button-constructor
                                      span.fa.fa-cube
                                      | Создать свою пиццу
            else
              .rest-catalog-dish_error Произошла ошибка блюд или блюда отсутствуют :(
